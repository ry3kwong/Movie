<%= form_for :movie, url: movies_path do |f| %>
<p>
	<%= f.label :title %> <br>
	<%= f.text_field :title %>
</p>

<p>
	<%= f.label :description %> <br>
	<%= f.text_field :description %>
</p>

<p>
	<%= f.label :year_released %> <br>
	<%= f.text_field :year_released %>
</p>



<p>
	<%= f.submit %>
</p>
<% end %>
