

<h1>Movies</h1>

<table class="table"> 
	<tr>
	 	<th>Title</th> 
		<th>Description</th> 
		<th>Year Released</th> 
		<th>Created By </th>
	
	

	</tr> 

	<% @movie.each do |movie| %> 
	<tr> 
		<td><%= link_to movie.title, movie_path(movie) %></td> 
		<td><%= movie.description %></td> 
		<td><%= movie.year_released %></td> 
		<td>Ryan</td>
	
	 </tr> 

	<% end %> 
</table>	
